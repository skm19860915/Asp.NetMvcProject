@model FeedbackRating

<div class="col-sm-4">
    <div class="panel panel-default">
        <div class="panel-heading">@Html.GlobalResource("OverallAverage")</div>
        <table class="table">
            <tr>
                <td><div class="stars-off"><div class="stars-on" style="width:@Math.Round(Model.AverageTotal * 16,0)px"></div></div></td>
                <td>@Math.Round(Model.AverageTotal, 2)</td>
            </tr>
            <tr>
                <td> <div class="stars-off"><div class="stars-on stars-on-5"></div></div></td>
                <td> @(Model.SellerFive + Model.BuyerFive)</td>
            </tr>
            <tr>
                <td><div class="stars-off"><div class="stars-on stars-on-4"></div></div></td>
                <td>@(Model.SellerFour + Model.BuyerFour)</td>
            </tr>
            <tr>
                <td><div class="stars-off"><div class="stars-on stars-on-3"></div></div></td>
                <td>@(Model.SellerThree + Model.BuyerThree)</td>
            </tr>
            <tr>
                <td><div class="stars-off"><div class="stars-on stars-on-2"></div></div></td>
                <td>@(Model.SellerTwo + Model.BuyerTwo)</td>
            </tr>
            <tr>
                <td><div class="stars-off"><div class="stars-on stars-on-1"></div></div></td>
                <td>@(Model.SellerOne + Model.BuyerOne)</td>
            </tr>
        </table>
    </div>
</div>
